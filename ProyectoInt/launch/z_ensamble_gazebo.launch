<?xml version="1.0" encoding="UTF-8"?>
<launch>	
	
	<!-- Object Model -->
    <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find ProyectoInt)/urdf/zona_ensamble.xacro'" />
	<arg name="robot_name" />
	<arg name="x" default="0"/>
	<arg name="y" default="0"/>
	<arg name="z" default="0"/>
	<arg name="Y" default="0"/>

	<!-- Spawn Robot Model -->
	<node name="mybot_spawn" 
		pkg="gazebo_ros" 
		type="spawn_model" 
		output="screen"
		args="-urdf -param robot_description
		-model $(arg robot_name) -x $(arg x) -y $(arg y) -z $(arg z) -Y $(arg Y)" />
   
</launch>